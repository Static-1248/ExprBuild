{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "ExprBuild DSL",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#operators"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [{
        "name": "keyword.control.exprbuild",
        "match": "\\b(expl|expr)\\b"
      }]
    },
    "operators": {
      "patterns": [{
        "name": "keyword.operator.exprbuild",
        "match": "[\\+\\-\\*/\\^=]"
      }]
    }
  },
  "scopeName": "source.exprbuild"
}